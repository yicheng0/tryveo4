# Cloudflare Environment Variables Template
# Copy these variables to your Cloudflare Pages dashboard

# Site Configuration
NODE_ENV=production
NEXT_PUBLIC_SITE_URL=https://tryveo4.pages.dev
NEXT_PUBLIC_OPTIMIZED_IMAGES=false
NEXT_PUBLIC_LOCALE_DETECTION=true
NEXT_PUBLIC_PRICING_PATH=/pricing

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# Stripe Configuration
STRIPE_SECRET_KEY=your_stripe_secret_key
STRIPE_WEBHOOK_SECRET=your_stripe_webhook_secret
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key

# AI Provider API Keys
OPENAI_API_KEY=your_openai_api_key
ANTHROPIC_API_KEY=your_anthropic_api_key
GOOGLE_GENERATIVE_AI_API_KEY=your_google_api_key
DEEPSEEK_API_KEY=your_deepseek_api_key
XAI_API_KEY=your_xai_api_key
OPENROUTER_API_KEY=your_openrouter_api_key
REPLICATE_API_TOKEN=your_replicate_token

# Cloudflare R2 Configuration
R2_PUBLIC_URL=your_r2_public_url
R2_ACCESS_KEY_ID=your_r2_access_key_id
R2_SECRET_ACCESS_KEY=your_r2_secret_access_key
R2_REGION=your_r2_region
R2_BUCKET_NAME=your_r2_bucket_name

# Email Configuration (Resend)
RESEND_API_KEY=your_resend_api_key
RESEND_FROM_EMAIL=your_from_email

# Rate Limiting (Upstash Redis)
UPSTASH_REDIS_REST_URL=your_upstash_redis_url
UPSTASH_REDIS_REST_TOKEN=your_upstash_redis_token

# Optional: Webhook processing
STRIPE_WEBHOOK_ASYNC_PROCESSING=false